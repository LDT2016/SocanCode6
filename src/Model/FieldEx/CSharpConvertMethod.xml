<?xml version="1.0" encoding="utf-8" ?>
<!--
DbType与自定义类型的对应关系，DbType属性请勿动，Value属性如有需要可自行修改
-->
<items>
  <item DbType="Byte" Value="Convert.ToByte"/>
  <item DbType="SByte" Value="Convert.ToSByte"/>
  <item DbType="Int16" Value="Convert.ToInt16"/>
  <item DbType="UInt16" Value="Convert.ToUInt16"/>
  <item DbType="Int32" Value="Convert.ToInt32"/>
  <item DbType="UInt32" Value="Convert.ToUInt32"/>
  <item DbType="Int64" Value="Convert.ToInt64"/>
  <item DbType="UInt64" Value="Convert.ToUInt64"/>
  
  <item DbType="Single" Value="Convert.ToSingle"/>
  <item DbType="Double" Value="Convert.ToDouble"/>
  
  <item DbType="Decimal" Value="Convert.ToDecimal"/>
  <item DbType="VarNumeric" Value="Convert.ToDecimal"/>
  <item DbType="Currency" Value="Convert.ToDecimal"/>
  
  <item DbType="Boolean" Value="Convert.ToBoolean"/>
  
  <item DbType="Guid" Value="new Guid"/>

  <item DbType="Date" Value="Convert.ToDateTime"/>
  <item DbType="Time" Value="Convert.ToDateTime"/>
  <item DbType="DateTime" Value="Convert.ToDateTime"/>

  <item DbType="Xml" Value="Convert.ToString"/>
  <item DbType="AnsiStringFixedLength" Value="Convert.ToString"/>
  <item DbType="AnsiString" Value="Convert.ToString"/>
  <item DbType="StringFixedLength" Value="Convert.ToString"/>
  <item DbType="String" Value="Convert.ToString"/>

  <item DbType="Binary" Value="Convert.ToByte"/>
  <item DbType="Object" Value=""/>
</items>
